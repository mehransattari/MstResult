<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>MST.Results</PackageId>
    <Authors>Mehran Sattari</Authors>
    <Company>MST</Company>
    <Product>MST.Results</Product>
    <Copyright>2024</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://mehransattary.ir/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mehransattari/MstResult</RepositoryUrl>
    <Description>Mehran Sattari Framework</Description>
    <PackageTags>Results</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
    <PackageIcon>Mst.png</PackageIcon>
    <PackageIconUrl />
    <PackageReleaseNotes>Use Results</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Update="Mst.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="MST" Version="1.0.0" />
    <PackageReference Include="FluentResults" Version="3.16.0" />
  </ItemGroup>

</Project>

